  AssertEqual 'VimwikiBold2'       , SyntaxAt(2, 14) . 2
  AssertEqual '3'                  , SyntaxAt(3, 14) . 3
  AssertEqual 'VimwikiBold4'       , SyntaxAt(4, 14) . 4
